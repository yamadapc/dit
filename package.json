{
  "name": "dit",
  "version": "0.0.1",
  "main": "index.js",
  "bin": "bin/dit",
  "description": "Reddit CLI utility",
  "homepage": "https://github.com/yamadapc/dit",
  "bugs": "https://github.com/yamadapc/dit/issues",
  "author": {
    "name": "Pedro Yamada",
    "email": "tacla.yamada@gmail.com",
    "url": "https://github.com/yamadapc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yamadapc/dit"
  },
  "licenses": [
    {
      "type": "GPL-2.0"
    }
  ],
  "scripts": {
    "test": "npm run jshint && npm run mocha",
    "jshint": "jshint lib/*.js lib/**/*.js bin/*",
    "mocha": "mocha -t 100000 -R mocha-spec-cov-alt --require blanket --compilers coffee:coffee-script/register"
  },
  "dependencies": {
    "bluebird": "~1.0.5",
    "lodash": "^2.4.1",
    "mime": "^1.2.11",
    "optimist": "^0.6.1",
    "read": "~1.0.5",
    "superagent": "~0.16.0"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "coffee-script": "^1.8.0",
    "coveralls": "^2.11.1",
    "jshint": "^2.5.5",
    "mocha": "^1.21.4",
    "mocha-make-stub": "^2.0.1",
    "mocha-spec-cov-alt": "^0.1.1",
    "should": "~3.1.2",
    "sinon": "~1.8.2"
  },
  "config": {
    "blanket": {
      "data-cover-never": "node_modules",
      "pattern": [
        "lib/",
        "bin/"
      ],
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80,
        "lcovOutput": "lcov"
      }
    }
  }
}
